{
  "name": "Codetmp",
  "icons": [{
    "src": "assets/images/128.png",
    "type": "image/png",
    "sizes": "128x128"
  }, {
    "src": "assets/images/192.png",
    "type": "image/png",
    "sizes": "192x192"
  }],
  "start_url": "/",
  "display": "standalone",
  "background_color": "#333",
  "theme_color": "#333",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/json": [".json"],
        "text/javascript": [".js"],
        "text/html": [".html"],
        "text/css": [".css"]
      },
      "icons": [
        {
          "src": "assets/images/128.png",
          "sizes": "128x128",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    }
  ]
}